{
  "version": 1,
  "description": "Cloudflare Pages routing - serve static files directly, run middleware for content pages",
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/sitemaps/*",
    "/sitemap.xml",
    "/sitemap-index.xml",
    "/ai-sitemap.xml",
    "/*.xml",
    "/*.txt",
    "/*.json",
    "/*.png",
    "/*.ico",
    "/*.jpg",
    "/*.jpeg",
    "/*.webp",
    "/*.svg",
    "/*.css",
    "/*.js",
    "/*.map",
    "/.well-known/*"
  ]
}
